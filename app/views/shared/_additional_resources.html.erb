<% if additional_resources.any? %>
  <!--p><strong><%= t('.title') %></strong-->
  <ul>
    <% additional_resources.each do |resource| %>
      <li><%= link_to resource.title, unbounded_resource_path(resource), class: 'additional-material-link', data: { 'resource-id' => resource.id, placement: 'right', toggle: 'tooltip' }, title: resource.title %>
    <% end %>
  </ul>
<% end %>
