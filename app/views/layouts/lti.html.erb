<!doctype html>
<html class="no-js" lang="">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script>document.querySelector('html').className = 'js';</script>
    <script>
      window._PDFJSworkerSrc = '<%= asset_url('vendor/pdf.worker.js') %>'
    </script>
    <%= stylesheet_link_tag 'main', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag '//s7.addthis.com/js/300/addthis_widget.js#async=1' %>
    <%= javascript_include_tag 'application_lti', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'pdf_js_preview', 'data-turbolinks-track' => true if generic_page? %>
    <%= csrf_meta_tags %>
    <% if content_for?(:canonical_url) %>
      <link rel=canonical href='<%= yield(:canonical_url) %>'>
    <% end %>
  </head>
  <body>
    <%= render partial: 'shared/unsupported' %>
    <div class="u-page-wrap u-supported<%= ' u-bg--light' if devise_controller? || controller.controller_name == 'surveys' %>">
      <%= yield %>
    </div>
  </body>
</html>
